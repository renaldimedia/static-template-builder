{
  "name": "static-starter",
  "description": "Static HTML/CSS/JS Template Build System",
  "main": "gulpfile.js",
  "author": "Roy Barber",
  "scripts": {
    "dev": "set ENV NODE_ENV=development LANG=en & netlify dev",
